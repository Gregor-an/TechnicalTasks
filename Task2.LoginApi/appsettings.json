{
    "ConnectionStrings": {
        "Default": "Data Source=login.db"
    },
    "Jwt": {
        "PublicKeyPath": "Keys/public.key.pem",
        "ValidateIssuer": false,
        "ValidateAudience": false,
        "ValidateLifetime": true,
        "ClockSkewSeconds": 30
    },
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "AllowedHosts": "*"
}
